{
  "name": "bw-home",
  "author": "Bram Willemse <contact@bramwillemse.nl>",
  "description": "Blog and website for Bram Willemse",
  "repository": "https://github.com/bramwillemse/bw-home.git",
  "bugs": {
    "url": "https://github.com/bramwillemse/bw-home/issues"
  },
  "homepage": "https://bramwillemse.nl",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "browserify": "browserify themes/bw-home/assets/js/main.js > themes/bw-home/assets/js/bundle.js",
    "watch": "watchify themes/bw-home/assets/js/main.js -o themes/bw-home/assets/js/bundle.js",
    "server": "hugo server --disableFastRender",
    "build": "hugo -v --gc --minify",
    "clean": "rimraf public"
  },
  "devDependencies": {
    "netlify-cli": "^2.19.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "babelify": "^10.0.0",
    "browserify": "*",
    "postcss": "^6.0.17",
    "postcss-custom-media": "*",
    "postcss-import": "*",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "*",
    "stylelint": "^13.3.3",
    "stylelint-order": "*",
    "vanilla-lazyload": "^12.0.0"
  }
}
